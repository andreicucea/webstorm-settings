<application>
  <component name="XDebuggerSettings">
    <data-views>
      <sort-values>true</sort-values>
    </data-views>
    <general>
      <option name="hideDebuggerOnProcessTermination" value="true" />
      <option name="showDebuggerOnBreakpoint" value="false" />
    </general>
    <debuggers>
      <debugger id="javascript">
        <configuration watchReturnValues="true">
          <custom-object-presentation />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>